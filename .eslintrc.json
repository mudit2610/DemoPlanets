{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": [
            2,
            {
                "extensions": [
                    ".js",
                    ".jsx"
                ]
            }
        ],
        "react/forbid-prop-types": [
            0
        ],
        "react/require-default-props": [
            0
        ],
        "global-require": [
            0
        ],
        "no-underscore-dangle": [
            0
        ],
        "arrow-parens": "off",
        "object-curly-newline": "off",
        "react/jsx-indent": [
            0
        ],
        "indent": "off"
    }
}